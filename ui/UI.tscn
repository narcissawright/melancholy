[sub_resource type="Shader" id=25]
code = "shader_type canvas_item;
render_mode unshaded;

uniform float outer_deadzone = 0.938;
uniform float inner_deadzone = 0.1225;
uniform float ramp = 2.0;

const vec3 full_input = vec3(1,1,1);
const vec3 no_input =   vec3(0,0,0);
const vec3 full_mix = vec3(1, 0.3, 0.3);
const vec3 no_mix = vec3 (0.3, 0.3, 1);

void fragment() {
	COLOR = texture(TEXTURE, UV);
	float len = length(UV - vec2(0.5, 0.5)) * 2.0;
	
	if (len > outer_deadzone) {
		COLOR.rgb = full_input;
	} else if (len < inner_deadzone) {
		COLOR.rgb = no_input;
	}  else {
		COLOR.rgb = mix(no_mix, full_mix, pow(len, ramp));
	}
	
	vec3 mix_amount = mix(no_input, full_input, len);
	
	// antialiasing
//	float aa_factor = fwidth(len);
//	float blend_amt = smoothstep(outer_deadzone - aa_factor, outer_deadzone, len);
//	COLOR.rgb = mix(mix_amount, full_input, blend_amt);
}"
custom_defines = ""

