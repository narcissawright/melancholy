[sub_resource type="Shader" id=1]
code = "shader_type canvas_item;
render_mode unshaded;

uniform vec4 fx_color : hint_color = vec4(0.4,0.3,0,0.5);

void fragment() {
	vec3 screen = textureLod(SCREEN_TEXTURE, SCREEN_UV, 0.0).rgb;
	float screen_brightness = clamp((screen.r + screen.g + screen.b) / 3.0, 0.0, 1.0);
	float fx_brightness = clamp((fx_color.r + fx_color.g + fx_color.b) / 3.0, 0.0, 1.0);
	if (screen_brightness > fx_brightness) {
		float inv_range = 1.0 / (1.0 - fx_brightness);
		float amt = inv_range * (screen_brightness - fx_brightness);
		COLOR.rgb = mix(fx_color.rgb, vec3(1,1,1), amt);
	} else {
		float amt = (screen_brightness / fx_brightness);
		COLOR.rgb = mix(vec3(0,0,0), fx_color.rgb, amt);
	}
	COLOR.a = fx_color.a;
}"

