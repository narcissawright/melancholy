[sub_resource type="Shader" id=25]
code = "shader_type canvas_item;
render_mode unshaded;

uniform vec2 stick_pos = vec2(0,0);
uniform float outer_threshold = 0.9;
uniform float axis_deadzone = 0.1;
uniform float ramp = 2.0;

const vec4 full_input = vec4(0.5,0.5,1.0,0.5);
const vec3 no_input =   vec3(0,0,0);
const vec3 full_mix = vec3(1, 0.3, 0.3);
const vec3 no_mix = vec3 (0.3, 0.3, 1);

void fragment() {
	COLOR = vec4(0,0,0,0.2);
	
	vec2 position = (UV - vec2(0.5, 0.5)) * 2.0;
	if (abs(position.x) < axis_deadzone) {
		if (abs(stick_pos.x) < axis_deadzone) {
			COLOR.a = 0.35;
		}
		COLOR.rgb += vec3(1.0, 0.5, 0.4);
	}
	
	if (abs(position.y) < axis_deadzone) {
		if (abs(stick_pos.y) < axis_deadzone) {
			COLOR.a = 0.35;
		}
		COLOR.rgb += vec3(0.5, 1.0, 0.4);
	}
	
	// Antialiasing for the circle.
	float len = length(position);
	float aa_factor = fwidth(len);
	float blend_amt = smoothstep(outer_threshold - aa_factor, outer_threshold, len);
	COLOR.rgb += vec3(0.5, 0.5, 1) * blend_amt;
	if (length(stick_pos) > outer_threshold) {
		COLOR.a = 0.5;
	}
}"

